<!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Add Technology
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-file-text"></i> khkhkh</a></li>
            <li class="active">Add</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">

          <div class="row">
            <!-- left column -->
            <div class="col-md-6">
              <!-- general form elements -->
              <div class="box box-primary">
                <div class="box-header">
                  <!--<h3 class="box-title">Quick Example</h3>-->
                </div><!-- /.box-header -->
                <!-- form start -->
                
				<div class="box-body">

					<!-- <div class="alert alert-danger alert-dismissable">
					    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
					    <h4><i class="icon fa fa-ban"></i> Alert!</h4>
					    sdsdsds
						</div>
					<div class="form-group">
					  <label>* xcxc</label>
					  <input type="text" class="form-control" name="id_user" id="id_user">
					</div>-->
					<%= form_for(@technology) do |f| %>
						<% if @technology.errors.any? %>
							<div class="alert alert-danger alert-dismissable">
		                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
		                        <h4><i class="icon fa fa-ban"></i> Alert!</h4>
		                        <% @technology.errors.full_messages.each do |message| %>
		                        * <%= message %> <br>
		                        <% end %>
                        	</div>
	                    <% end %>
						
						<div class="form-group">
							<%= f.label :name, 'Name' %>
							<%= f.text_field :name, placeholder: "Name", class: "form-control" %>
						</div>
						<div class="form-group">
							<%= f.label :description, 'Description' %>
							<%= f.text_area :description, placeholder: "Description", class: "form-control" %>
						</div>
						<div class="form-group">
							<%= f.label :version, 'Versión' %>
							<%= f.text_field :version, placeholder: "Versión", class: "form-control" %>
						</div>
						<div class="form-group">
							<%= f.label :link, 'Link' %>
							<%= f.text_field :link, placeholder: "Link", class: "form-control" %>
						</div>
						<div class="box-footer">
							<%= f.submit "Save", class: "btn btn-primary" %>
						</div>
					<% end %>
				</div><!-- /.box-body -->
              </div><!-- /.box -->
            </div>
          </div>   <!-- /.row -->

        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->