        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Edit Technology
          </h1>
          <ol class="breadcrumb">
            <li>
            	<%= link_to technologies_path(@technology) do %>
	              <i class="fa fa-github"></i> Technologies
	            <% end %>
            </li>
            <li class="active">Edit</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">

          <div class="row">
            <!-- left column -->
